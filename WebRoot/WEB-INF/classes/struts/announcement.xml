<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="announcement" extends="struts-default" namespace="/announcement">

		<action name="announcement_*" class="com.hust.action.annoucement.AnnouncementAction"
			method="{1}">
			<result name="error">/WEB-INF/page/error.jsp</result>
			<result name="goAnnouncementManage">/WEB-INF/page/xtgl/announcementmanage.jsp</result>					
			<result name="getAnnouncementByKeyword" >
				/display.jsp
			</result>	
			<result name="addAnnouncement">
				/display.jsp
			</result>	
			<result name="editAnnouncement">
				/display.jsp
			</result>
			<result name="deleteAnnouncement">
				/display.jsp
			</result>	
		</action>
	</package>
</struts>
