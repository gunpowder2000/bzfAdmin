<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="question" extends="struts-default" namespace="/question">

		<action name="question_*" class="com.hust.action.question.QuestionAction"
			method="{1}">
			<result name="error">/WEB-INF/page/error.jsp</result>
			<result name="goQuestionManage">/WEB-INF/page/sqgl/questionmanage.jsp</result>			
			<result name="searchQuestion">
				/display.jsp
			</result>
			<result name="answerQuestion">
				/display.jsp
			</result>			
		</action>
	</package>
</struts>
